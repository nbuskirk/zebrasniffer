{
  "name" : "ZebraSniffer",
  "version" : "1.50",
  "description" : "Apica ZebraTester Recorder Toolbar",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "options_page": "options.html",
  "permissions": [
    "browsingData",
    "storage",
    "background",
    "tabs",
    "proxy",
    "notifications",
    "http://*/",
    "https://api-wpm.apicasystem.com/"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+Z",
        "mac": "Command+Shift+Z",
        "chromeos": "Ctrl+Shift+Z",
        "linux": "Ctrl+Shift+Z"
      }
    }
  },
  "icons": { 
    "48": "icon48.png",
    "24": "iconlg.png"
  },
  "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html",
     "default_title": "ZebraSniffer"
  },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": true
  },
  "manifest_version": 2
}
